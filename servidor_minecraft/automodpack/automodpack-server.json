  "nagClickableLink": "https://modrinth.com/project/automodpack",
  "autoExcludeServerSideMods": true,
  "hostModpackOnMinecraftPort": true,
  "hostIp": "",
  "hostLocalIp": "",
  "updateIpsOnEveryStart": false,
  "hostPort": -1,
  "reverseProxy": false,
  "selfUpdater": false,
  "acceptedLoaders": [
    "forge"
  ]
