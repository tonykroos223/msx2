{
  "DO_NOT_CHANGE_IT": 1,
  "modpackName": "",
  "modpackHost": true,
  "generateModpackOnStart": true,
  "syncedFiles": [
    "/mods/*.jar",
    "!/mods/iDontWantThisModInModpack.jar",
    "!/config/andThisConfigToo.json",
    "!/mods/andAllTheseMods-*.jar",
    "!/mods/server-*.jar"
